    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.42.1",
    "stylelint": "^13.13.1",
    "svelte": "^3.34.0",
    "svelte-preprocess": "^4.9.5"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
